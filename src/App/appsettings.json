{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Verbose",
        "System": "Verbose"
      }
    },
    "WriteTo": [
      {
        "Name": "Trace",
        "Args": {
          "restrictedToMinimumLevel": "Debug",
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level}] {SourceContext:l} {Account:l} {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "RollingFile",
        "Args": {
          "restrictedToMinimumLevel": "Debug",
          "pathFormat": "..\\logs\\{Date}.log",
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level}] {SourceContext:l} {Account:l} {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}
